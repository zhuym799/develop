<ActionTemplates>
   <Action>
      <Name>Service.Continue</Name>
      <Description>继续指定的服务。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>DisplayName</Name>
            <Description>用于查找服务的显示名称。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"ServiceDisplayName"</Default>
               <DataType>string</DataType>
               <Constraints>0,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>KeyName</Name>
            <Description>在无法通过显示名称找到服务时要采用的键名。</Description>
            <Type>string</Type>
            <Default>""</Default>
            <Required>o</Required>
            <EasyMode>
               <Default>""</Default>
               <DataType>string</DataType>
               <Constraints>0,*</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>Service.Create</Name>
      <Description>在本地系统中创建服务。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>Filename</Name>
            <Description>服务二进制文件的完整路径 (带有或不带有参数)。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"C:\\Windows\\Service.exe"</Default>
               <DataType>string</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>DisplayName</Name>
            <Description>用户接口程序用来识别服务的字符串。</Description>
            <Type>string</Type>
            <Default>"服务1"</Default>
            <Required>1</Required>
            <EasyMode>
               <Default/>
               <DataType>string</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>KeyName</Name>
            <Description>系统用来安装服务的字符串。</Description>
            <Type>string</Type>
            <Default>""</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>""</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>ServiceType</Name>
            <Description>服务类型。</Description>
            <Type>number</Type>
            <Default>SERVICE_WIN32_OWN_PROCESS</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>SERVICE_WIN32_OWN_PROCESS</Default>
               <DataType>combo</DataType>
               <Constraints>SERVICE_WIN32_OWN_PROCESS,SERVICE_WIN32_SHARE_PROCESS,SERVICE_KERNEL_DRIVER,SERVICE_FILE_SYSTEM_DRIVER</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>AllowDesktopInteraction</Name>
            <Description>启用服务的桌面交互。</Description>
            <Type>boolean</Type>
            <Default>false</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>false</Default>
               <DataType>combo</DataType>
               <Constraints>true,false</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>StartType</Name>
            <Description>指定服务启动的时间。</Description>
            <Type>number</Type>
            <Default>SERVICE_DEMAND_START</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>SERVICE_DEMAND_START</Default>
               <DataType>combo</DataType>
               <Constraints>SERVICE_BOOT_START,SERVICE_SYSTEM_START,SERVICE_AUTO_START,SERVICE_DEMAND_START,SERVICE_DISABLED</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>ErrorControl</Name>
            <Description>指定服务启动失败时的错误信息。</Description>
            <Type>number</Type>
            <Default>SERVICE_ERROR_NORMAL</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>SERVICE_ERROR_NORMAL</Default>
               <DataType>combo</DataType>
               <Constraints>SERVICE_ERROR_IGNORE,SERVICE_ERROR_NORMAL,SERVICE_ERROR_SEVERE,SERVICE_ERROR_CRITICAL,SERVICE_ERROR_NORMAL</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>LoadOrderGroup</Name>
            <Description>成员服务的加载顺序组的名称。</Description>
            <Type>string</Type>
            <Default>""</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>0</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>GroupTag</Name>
            <Description>只在加载顺序组时才有的标签值。</Description>
            <Type>number</Type>
            <Default>nil</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>nil</Default>
               <DataType>number</DataType>
               <Constraints>0,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Dependencies</Name>
            <Description>包含必须在此服务启动之前加载的服务名称的数字索引表。</Description>
            <Type>table</Type>
            <Default>nil</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>nil</Default>
               <DataType>table</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>ServiceStartName</Name>
            <Description>此服务名称的字符串，其值依赖于服务类型。</Description>
            <Type>string</Type>
            <Default>""</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>""</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Password</Name>
            <Description>用于服务的密码。</Description>
            <Type>string</Type>
            <Default>""</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>""</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
