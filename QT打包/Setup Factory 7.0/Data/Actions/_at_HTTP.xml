<ActionTemplates>
   <Action>
      <Name>HTTP.Download</Name>
      <Description>从网站上下载文件。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>URL</Name>
            <Description>文件的完整 URL。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"http://www.mydomain.com/myfile.exe"</Default>
               <DataType>string</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Filename</Name>
            <Description>下载后要保存的文件名。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"C:\\Downloads\\myfile.exe"</Default>
               <DataType>proj_folder</DataType>
               <Constraints/>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Mode</Name>
            <Description>文件的格式化模式。</Description>
            <Type>number</Type>
            <Default>MODE_BINARY</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>MODE_BINARY</Default>
               <DataType>combo</DataType>
               <Constraints>MODE_BINARY,MODE_TEXT</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Timeout</Name>
            <Description>连接超时。</Description>
            <Type>number</Type>
            <Default>20</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>20</Default>
               <DataType>number</DataType>
               <Constraints>0,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Port</Name>
            <Description>要使用的端口，通常应为 80。</Description>
            <Type>number</Type>
            <Default>80</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>80</Default>
               <DataType>number</DataType>
               <Constraints>0,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>AuthData</Name>
            <Description>将被用于访问文件的 HTTP 基本验证信息的表格。</Description>
            <Type>table</Type>
            <Default>nil</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>nil</Default>
               <DataType>table</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>ProxyData</Name>
            <Description>将被用于访问文件的代理信息的表格。你可以通过 nil (默认) 来使用当前 IE 中配置的默认代理设置。</Description>
            <Type>table</Type>
            <Default>nil</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>nil</Default>
               <DataType>table</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>CallbackFunction</Name>
            <Description>在不需要默认的进度操作时将被调用的函数 (带有进度信息)。</Description>
            <Type>function</Type>
            <Default>nil</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>nil</Default>
               <DataType>function</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
