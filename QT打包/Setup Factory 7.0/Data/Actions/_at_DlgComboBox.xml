<ActionTemplates>
   <Action>
      <Name>DlgComboBox.AddItem</Name>
      <Description>将项目添加到已排序的组合框控件。如果控件未排序，项目将被添加到末尾。</Description>
      <ReturnValueType>number</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>ControlID</Name>
            <Description>组合框控件的数字 ID。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default/>
               <DataType>ComboBox</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>ItemText</Name>
            <Description>要添加的项目文本。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
            	<Default>"样例文本"</Default>
            	<DataType>string</DataType>
           	 <Constraints>0,*</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>DlgComboBox.GetItemData</Name>
      <Description>获取当前屏幕上组合框控件中项目的关联数据字符串。</Description>
      <ReturnValueType>string</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>ControlID</Name>
            <Description>组合框控件的数字 ID。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default/>
               <DataType>ComboBox</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Index</Name>
            <Description>组合框项目的索引 (行号)。列表中的第一项的索引为 1。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>1</Default>
               <DataType>number</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>DlgComboBox.GetItemText</Name>
      <Description>返回当前屏幕上组合框控件中项目的文本。</Description>
      <ReturnValueType>string</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>ControlID</Name>
            <Description>组合框控件的数字 ID。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default/>
               <DataType>ComboBox</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Index</Name>
            <Description>组合框项目的索引 (行号)。列表中的第一项的索引为 1。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>1</Default>
               <DataType>number</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>DlgComboBox.GetProperties</Name>
      <Description>获取当前屏幕上组合框控件的属性。</Description>
      <ReturnValueType>table</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>ControlID</Name>
            <Description>组合框控件的数字 ID。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default/>
               <DataType>ComboBox</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>DlgComboBox.InsertItem</Name>
      <Description>插入项目到组合框控件。</Description>
      <ReturnValueType>number</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>ControlID</Name>
            <Description>组合框控件的数字 ID。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default/>
               <DataType>ComboBox</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Index</Name>
            <Description>插入新项目的索引 (行号)。使用索引 -1 插入到列表末尾。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>-1</Default>
               <DataType>number</DataType>
               <Constraints>*,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>ItemText</Name>
            <Description>新项目要显示的项目文本。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"样例文本"</Default>
               <DataType>string</DataType>
               <Constraints>0,*</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>DlgComboBox.RemoveItem</Name>
      <Description>从组合框控件中移除项目。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>ControlID</Name>
            <Description>组合框控件的数字 ID。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default/>
               <DataType>ComboBox</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Index</Name>
            <Description>要从组合框中移除的项目的索引 (行号)。列表中第一项的索引为 1。你可以使用索引 -1 来移除列表中的所有项目。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>-1</Default>
               <DataType>number</DataType>
               <Constraints>*,*</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
      <Action>
      <Name>DlgComboBox.SetItemData</Name>
      <Description>将组合框控件的项目设置为字符串，以便今后能被检索。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>ControlID</Name>
            <Description>组合框控件的数字 ID。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default/>
               <DataType>ComboBox</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Index</Name>
            <Description>组合框项目的索引 (行号)。列表中第一项的索引为 1。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>1</Default>
               <DataType>number</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>ItemData</Name>
            <Description>要与项目数据关联的文本。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"项目数据字符串"</Default>
               <DataType>string</DataType>
               <Constraints>0,*</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
      <Action>
      <Name>DlgComboBox.SetProperties</Name>
      <Description>设置当前屏幕上组合框控件的属性。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>ControlID</Name>
            <Description>组合框控件的数字 ID。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default/>
               <DataType>ComboBox</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Properties</Name>
            <Description>包含由 "Enabled", "Text" 和 "Visible" 索引的组合框控件属性的表格。</Description>
            <Type>table</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>my_table</Default>
               <DataType>table</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
</ActionTemplates>
