<ActionTemplates>
   <Action>
      <Name>Table.Concat</Name>
      <Description>返回包含表中某些或全部值的字符串。这些值必须由定界符分隔。</Description>
      <ReturnValueType>string</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>SourceTable</Name>
            <Description>要连接值的表。</Description>
            <Type>table</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>my_table</Default>
               <DataType>table</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Separator</Name>
            <Description>用来分隔表值的定界符。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>";"</Default>
               <DataType>string</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Start</Name>
            <Description>表中第一项的索引。</Description>
            <Type>number</Type>
            <Default>1</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>1</Default>
               <DataType>number</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>End</Name>
            <Description>表中最后一项的索引。</Description>
            <Type>number</Type>
            <Default>TABLE_ALL</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>TABLE_ALL</Default>
               <DataType>number</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>Table.Count</Name>
      <Description>返回表中的项目总数。</Description>
      <ReturnValueType>number</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>SourceTable</Name>
            <Description>要获取项目数量的表。</Description>
            <Type>table</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>my_table</Default>
               <DataType>table</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>Table.Sort</Name>
      <Description>排序数字索引表的项目。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>SourceTable</Name>
            <Description>要排序的表。</Description>
            <Type>table</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>my_table</Default>
               <DataType>table</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Compare</Name>
            <Description>排序表时要使用的可选比较函数。如果没有指定函数，小于号 (&lt;) 将被用来排序项目。</Description>
            <Type>function</Type>
            <Default>nil</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>nil</Default>
               <DataType>function</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>Table.Insert</Name>
      <Description>将项目插入到数字索引表的指定位置。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>SourceTable</Name>
            <Description>要插入项目的表。</Description>
            <Type>table</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>my_table</Default>
               <DataType>table</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Position</Name>
            <Description>要插入项目的位置索引。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>1</Default>
               <DataType>number</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Value</Name>
            <Description>要插入到表中的项目。</Description>
            <Type>variant</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>1</Default>
               <DataType>number</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>Table.Remove</Name>
      <Description>从数字索引表中移除指定项目，并返回其值。</Description>
      <ReturnValueType>variant</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>SourceTable</Name>
            <Description>要移除项目的表。</Description>
            <Type>table</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>my_table</Default>
               <DataType>table</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Position</Name>
            <Description>要移除项目的位置索引。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>1</Default>
               <DataType>number</DataType>
               <Constraints>1,*</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
</ActionTemplates>
