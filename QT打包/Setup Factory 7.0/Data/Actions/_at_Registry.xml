<ActionTemplates>
   <Action>
      <Name>Registry.CreateKey</Name>
      <Description>在用户的注册表中创建“键”。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>MainKey</Name>
            <Description>要创建键的主键或“根”键。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>HKEY_LOCAL_MACHINE</Default>
               <DataType>combo</DataType>
               <Constraints>HKEY_CLASSES_ROOT,HKEY_CURRENT_CONFIG,HKEY_CURRENT_USER,HKEY_LOCAL_MACHINE,HKEY_USERS</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>SubKey</Name>
            <Description>要创建的子键。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"Software\\My Application"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>Registry.DeleteKey</Name>
      <Description>从用户的注册表中删除“键”。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>MainKey</Name>
            <Description>要删除键的主键或“根”键。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>HKEY_LOCAL_MACHINE</Default>
               <DataType>combo</DataType>
               <Constraints>HKEY_CLASSES_ROOT,HKEY_CURRENT_CONFIG,HKEY_CURRENT_USER,HKEY_LOCAL_MACHINE,HKEY_USERS</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>SubKey</Name>
            <Description>要删除的子键。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"Software\\My Application"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>Registry.DeleteValue</Name>
      <Description>从用户的注册表中删除“值”。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>MainKey</Name>
            <Description>值所在的主键或“根”键。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>HKEY_LOCAL_MACHINE</Default>
               <DataType>combo</DataType>
               <Constraints>HKEY_CLASSES_ROOT,HKEY_CURRENT_CONFIG,HKEY_CURRENT_USER,HKEY_LOCAL_MACHINE,HKEY_USERS</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>SubKey</Name>
            <Description>包含值的子键。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"Software\\My Application"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Value</Name>
            <Description>要删除的值。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"MyValue"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>Registry.DoesKeyExist</Name>
      <Description>确定是否存在指定的注册表键，并返回 true 或 false。</Description>
      <ReturnValueType>boolean</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>MainKey</Name>
            <Description>包含子键的主键或“根”键。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>HKEY_LOCAL_MACHINE</Default>
               <DataType>combo</DataType>
               <Constraints>HKEY_CLASSES_ROOT,HKEY_CURRENT_CONFIG,HKEY_CURRENT_USER,HKEY_LOCAL_MACHINE,HKEY_USERS</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>SubKey</Name>
            <Description>要检查是否存在的子键。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"Software\\My Application"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>Registry.GetAccess</Name>
      <Description>检查注册表键是否有指定的访问权限，并返回 true 或 false。</Description>
      <ReturnValueType>boolean</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>MainKey</Name>
            <Description>包含子键的主键或“根”键。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>HKEY_LOCAL_MACHINE</Default>
               <DataType>combo</DataType>
               <Constraints>HKEY_CLASSES_ROOT,HKEY_CURRENT_CONFIG,HKEY_CURRENT_USER,HKEY_LOCAL_MACHINE,HKEY_USERS</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>SubKey</Name>
            <Description>要检查访问权限的子键。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"Software\\My Application"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Rights</Name>
            <Description>要检查的访问权限。</Description>
            <Type>number</Type>
            <Default>ACCESS_ALL</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>ACCESS_ALL</Default>
               <DataType>combo</DataType>
               <Constraints>ACCESS_READ,ACCESS_WRITE,ACCESS_ENUMERATE,ACCESS_ALL</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>Registry.GetKeyNames</Name>
      <Description>给指定的键返回包含所有子键名称的表格。</Description>
      <ReturnValueType>table</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>MainKey</Name>
            <Description>包含子键的主键或“根”键。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>HKEY_LOCAL_MACHINE</Default>
               <DataType>combo</DataType>
               <Constraints>HKEY_CLASSES_ROOT,HKEY_CURRENT_CONFIG,HKEY_CURRENT_USER,HKEY_LOCAL_MACHINE,HKEY_USERS</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>SubKey</Name>
            <Description>要获取其子键名称的键。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"Software\\My Application"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
   <Action>
      <Name>Registry.GetValue</Name>
      <Description>返回指定注册表值的数据。</Description>
      <ReturnValueType>string</ReturnValueType>
      <Arguments>
         <Arg>
            <Name>MainKey</Name>
            <Description>包含子键的主键或“根”键。</Description>
            <Type>number</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>HKEY_LOCAL_MACHINE</Default>
               <DataType>combo</DataType>
               <Constraints>HKEY_CLASSES_ROOT,HKEY_CURRENT_CONFIG,HKEY_CURRENT_USER,HKEY_LOCAL_MACHINE,HKEY_USERS</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>SubKey</Name>
            <Description>包含值的子键。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"Software\\My Application"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Value</Name>
            <Description>要获取数据的值。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"MyValue"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>AutoExpand</Name>
            <Description>是否在返回数据之前展开其中的环境变量。此选项用在检索类型为 REG_EXPAND_SZ 的值时。</Description>
            <Type>boolean</Type>
            <Default>true</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>true</Default>
               <DataType>boolean</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
