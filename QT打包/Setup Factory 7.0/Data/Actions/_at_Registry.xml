            <Required>1</Required>
            <EasyMode>
               <Default>"MyValue"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Data</Name>
            <Description>要分配给值的数据。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"我的数据"</Default>
