<ActionTemplates>
   <Action>
      <Name>INIFile.DeleteSection</Name>
      <Description>删除指定 INI 文件中的“区段”。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>Filename</Name>
            <Description>INI 文件的路径。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>_WindowsFolder.."\\My Settings.ini"</Default>
               <DataType>fileedit</DataType>
               <Constraints>Docs</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Section</Name>
            <Description>要删除的 INI 文件区段。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"我的区段"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
