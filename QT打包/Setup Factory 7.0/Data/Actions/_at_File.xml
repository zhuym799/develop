<ActionTemplates>
   <Action>
      <Name>File.Copy</Name>
      <Description>复制文件到其他位置。</Description>
      <ReturnValueType/>
      <Arguments>
         <Arg>
            <Name>Source</Name>
            <Description>文件的路径。你可以使用 * 和 ? 通配符来复制多个文件。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"C:\\MyDir\\*.*"</Default>
               <DataType>fileedit</DataType>
               <Constraints>Docs</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Destination</Name>
            <Description>目标文件夹或文件的路径。</Description>
            <Type>string</Type>
            <Default/>
            <Required>1</Required>
            <EasyMode>
               <Default>"C:\\DestDir\\"</Default>
               <DataType>string</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Recurse</Name>
            <Description>当源参数中使用通配符时，是否递归子文件夹。</Description>
            <Type>boolean</Type>
            <Default>true</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>true</Default>
               <DataType>boolean</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>Overwrite</Name>
            <Description>如果存在同名文件，是否覆盖。</Description>
            <Type>boolean</Type>
            <Default>true</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>true</Default>
               <DataType>boolean</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>AbortOnFail</Name>
            <Description>如果发生错误，是否中止复制。</Description>
            <Type>boolean</Type>
            <Default>false</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>false</Default>
               <DataType>boolean</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>IncludeHiddenFiles</Name>
            <Description>是否复制具有“隐藏”或“系统”属性的文件。</Description>
            <Type>boolean</Type>
            <Default>true</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>true</Default>
               <DataType>boolean</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
         <Arg>
            <Name>CallbackFunction</Name>
            <Description>将在复制过程中被调用的函数名称。</Description>
            <Type>function</Type>
            <Default>nil</Default>
            <Required>0</Required>
            <EasyMode>
               <Default>nil</Default>
               <DataType>function</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
      </Arguments>
   </Action>
